<% if (typeof element === 'undefined') { element = []; }
  const returnNewsLabel = (number) => {
    switch (number) {
      case 1:
        return "トピックス";
        break;
      case 2:
        return "イベント・キャンペーン";
        break;
      case 3:
        return "入庫車情報";
        break;
      default:
        return "その他";
      }
  }
%>
<li class="list">
  <div class="list__upperContents">
    <span class="newsDate"><%- element.newsDate %></span>
    <ul class="newsLabels">
      <% const labels = element.newsLabels || [];
      labels.forEach((label) => { %>
      <li class="newsLabel"><%- returnNewsLabel(label) %></li>
      <% }) %>
    </ul>
  </div>
  <div class="list__text"><%- element.newsText %></div>
</li>